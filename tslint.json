{
    "extends": [
      "tslint:recommended",
      "tslint-react",
      "tslint-config-prettier"
    ],
    "rulesDirectory": [
      "tslint-plugin-prettier"
    ],
    "rules": {
      "prettier": true,
      "interface-name": false,
      "node/no-missing-import": "off",
			"import/prefer-default-export": "off",
			"import/no-default-export": "error",
			"sort-imports": "off",
			"import/order": "off"
  }
}
